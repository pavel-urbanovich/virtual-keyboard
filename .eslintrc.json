{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "import/extensions": ["error", "ignorePackages", {
            "js": "always",
            "jsx": "never",
            "mjs": "never"
          }]
    }
}
